<div id="<%= dom_id pet %>">
  <p>
    <strong>User:</strong>
    <%= pet.user_id %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= pet.name %>
  </p>

  <p>
    <strong>Birth date:</strong>
    <%= pet.birth_date %>
  </p>

  <p>
    <strong>Species:</strong>
    <%= pet.species %>
  </p>

</div>
